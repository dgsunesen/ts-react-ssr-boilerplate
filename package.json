{
  "name": "React-Redux-Boilerplate",
  "version": "4.0.0",
  "description": "A starter boilerplate to create comprehensive React apps with Redux and serverside rendering.",
  "author": "sandervispoel <contact@sandervispoel.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/sandervspl/react-redux-typescript-styled-components-boilerplate"
  },
  "license": "MIT",
  "scripts": {
    "start": "node devServer.js",
    "build": "NODE_ENV=production run-p clean build:client build:server",
    "build:client": "webpack --hide-modules --config webpack.config.prod.js",
    "build:server": "webpack -p --hide-modules --config webpack.config.server.js",
    "server": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint src/"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "typescript",
    "styled-components"
  ],
  "dependencies": {
    "@types/qs": "^6.5.1",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.1",
    "styled-normalize": "^2.2.1",
    "svg-react-loader": "^0.4.5",
    "uglifyjs-webpack-plugin": "^1.2.2"
  },
  "devDependencies": {
