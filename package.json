    "rimraf": "^2.6.2",
    "ts-loader": "^4.0.1",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.7.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.1.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": "8.5.0"
  }
}
