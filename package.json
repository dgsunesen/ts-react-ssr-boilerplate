{
  "name": "React-Redux-Boilerplate",
  "version": "4.0.0",
  "description": "A starter boilerplate to create comprehensive React apps with Redux and serverside rendering.",
  "author": "jornbostelaar",
  "repository": {
    "type": "git",
    "url": "https://github.com/JBostelaar/react-redux-boilerplate"
  },
  "license": "MIT",
  "scripts": {
    "start": "node devServer.js",
    "build": "NODE_ENV=production run-p clean build:client build:server",
    "build:client": "webpack -p --hide-modules --config webpack.config.prod.js",
    "build:server": "webpack -p --hide-modules --config webpack.config.server.js",
    "server": "node dist/server.js",
    "clean": "rimraf dist/",
    "lint": "eslint src/"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel"
  ],
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.2.3",
    "styled-normalize": "^2.2.1",
    "svg-react-loader": "^0.4.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.78",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^5.0.10",
    "@types/react-router-dom": "^4.0.8",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "file-loader": "^1.1.5",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-node-externals": "^1.6.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0"
    ],
    "plugins": [
      "lodash"
    ]
  },
  "engines": {
    "node": "8.5.0"
  }
}
